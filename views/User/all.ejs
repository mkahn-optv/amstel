<div class="container">
    <h3>Users</h3>
    <table class="table">
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th></th>
            <th></th>
            <th></th>
        </tr>
        <% _.each(users, function (user) { %>
        <tr data-id="<%= user.id %>" data-model="user">
            <td><%= user.id %></td>
            <td><%= user.firstName %>&nbsp;<%= user.lastName %></td>
            <td><%= user.email %></td>
            <td><a class="btn btn-sm btn-primary" href="/user/show/<%= user.id %>">Show</a></td>
            <td><a class="btn btn-sm btn-warning" href="/user/edit/<%= user.id %>">Edit</a></td>
            <td><a class="btn btn-sm btn-danger" href="/user/delete/<%= user.id %>">Delete</a></td>
        </tr>
        <% })%>
    </table>
</div>